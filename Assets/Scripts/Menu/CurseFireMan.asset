    \u043D\u0430\u0434 \u043E\u0433\u043D\u0451\u043C, \u0447\u0442\u043E \u043F\u043E\u0437\u0432\u043E\u043B\u0438\u043B
    \u0441\u0435\u0431\u0435 \u0441\u0433\u043E\u0440\u0435\u0442\u044C \u0437\u0430\u0436\u0438\u0432\u043E,
    \u043B\u0438\u0448\u044C \u0431\u044B \u0441\u043B\u0438\u0442\u044C\u0441\u044F
    \u0441 \u043F\u043B\u0430\u043C\u0435\u043D\u0435\u043C. \u0415\u0433\u043E \u043A\u0440\u0438\u043A\u0438
    \u043D\u0435 \u0443\u0442\u0438\u0445\u043B\u0438 \u2014 \u043E\u043D\u0438 \u0437\u0430\u043F\u0435\u0447\u0430\u0442\u043B\u0435\u043D\u044B
    \u0432 \u044D\u0442\u043E\u043C \u043F\u0440\u043E\u043A\u043B\u044F\u0442\u0438\u0438.
    \u041F\u0440\u0438 \u043D\u0430\u043B\u043E\u0436\u0435\u043D\u0438\u0438 \u043F\u043E\u0440\u0447\u0438,
    \u043F\u043E \u043B\u0435\u0437\u0432\u0438\u044E \u0440\u0430\u0441\u043F\u043E\u043B\u0437\u0430\u044E\u0442\u0441\u044F
    \u0447\u0451\u0440\u043D\u044B\u0435 \u0442\u0440\u0435\u0449\u0438\u043D\u044B,
    \u0438\u0437 \u043A\u043E\u0442\u043E\u0440\u044B\u0445 \u0441\u043E\u0447\u0438\u0442\u0441\u044F
    \u0442\u0443\u0441\u043A\u043B\u043E\u0435, \u0431\u0430\u0433\u0440\u043E\u0432\u043E\u0435
    \u043F\u043B\u0430\u043C\u044F. \u041C\u0435\u0447 \u043D\u0430\u0447\u0438\u043D\u0430\u0435\u0442
    \u043F\u0435\u0442\u044C \u0433\u043B\u0443\u0445\u0438\u043C, \u0442\u043B\u0435\u044E\u0449\u0438\u043C
    \u0437\u0432\u0443\u043A\u043E\u043C \u043F\u0440\u0438 \u043A\u0430\u0436\u0434\u043E\u043C
    \u0437\u0430\u043C\u0430\u0445\u0435, \u0430 \u043F\u0440\u0438 \u0443\u0434\u0430\u0440\u0435
    \u043F\u043E \u043F\u043B\u043E\u0442\u0438 \u0432\u0440\u0430\u0433\u0430 \u043F\u043E\u0440\u0447\u0430
    \u043F\u0440\u043E\u043D\u0438\u043A\u0430\u0435\u0442 \u0432\u043D\u0443\u0442\u0440\u044C,
    \u043E\u0431\u0440\u0430\u0449\u0430\u044F \u043A\u0440\u043E\u0432\u044C \u0432
